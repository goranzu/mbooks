{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "User",
  "description": "User",
  "required": ["username", "password"],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "Users id"
    },
    "username": {
      "$id": "#/properties/username",
      "type": "string",
      "title": "Username",
      "examples": ["Alaina_Hand25"]
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "Users hashed password"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "Date when the user was registered"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was updated"
    },
    "last_login": {
      "$id": "#/properties/last_login",
      "type": "string",
      "title": "The date the user logged in the last time"
    }
  },
  "additionalProperties": false
}
