{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Book",
  "description": "Book",
  "required": ["goodreads_id", "title", "author"],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "Book id"
    },
    "goodreads_id": {
      "$id": "#/properties/goodreads_id",
      "type": "string",
      "title": "Id provided by the goodreads api"
    },
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "Book Title"
    },
    "author": {
      "$id": "#/properties/author",
      "type": "string",
      "title": "Book Author"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "Book description"
    },
    "image_url": {
      "$id": "#/properties/image_url",
      "type": "string",
      "title": "Url for the cover image of the book"
    },
    "average_rating": {
      "$id": "#/properties/average_rating",
      "type": "string",
      "title": "Average rating of the book"
    },
    "publication_year": {
      "$id": "#/properties/publication_year",
      "type": "string",
      "title": "The year the book was published"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "Date when the book was addedd"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the book info was updated"
    }
  },
  "additionalProperties": false
}
